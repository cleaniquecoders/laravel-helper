{
	"name": "cleaniquecoders/laravel-helper",
	"description": "Built with Laravel Standalone Package Creator",
	"keywords": [
		"package",
		"laravel"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Nasrul Hazim",
			"email": "nasrulhazim.m@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"CleaniqueCoders\\LaravelHelper\\": "src/"
		},
		"files": [
			"src/Support/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"CleaniqueCoders\\LaravelHelper\\Tests\\": "tests/"
		}
	},
	"require": {
		"php": "^8.1 | ^8.2 | ^8.3",
		"illuminate/support": "^9.0 | ^10.0 | ^11.0"
	},
	"require-dev": {
		"laravel/pint": "^1.6",
		"mockery/mockery": "^1.1",
		"orchestra/testbench": "8.*|9.*",
		"pestphp/pest": "^2.0",
		"phpunit/phpunit": "^9.0 | ^10.0 | ^11.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"CleaniqueCoders\\LaravelHelper\\LaravelHelperServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"scripts": {
		"test": "vendor/bin/pest",
		"format": "vendor/bin/pint"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
